import sympy
import sympytex
sympytex.openout('main')
sympytex.blockbegin()
try:
 r,c,t,theta = sympy.symbols('r c t theta')
 
 eq1 = 2 * r * sympy.cos(theta) - c
 eq2 = r - r * sympy.sin(theta) - t
 sol = sympy.solve([eq1,eq2],[r,theta])
 
except:
 sympytex.goboom(88)
sympytex.blockend()
try:
 sympytex.inline(0, sol)
except:
 sympytex.goboom(92)
sympytex.endofdoc()
